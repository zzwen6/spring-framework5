<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="org.springframework.practise.User">
		<constructor-arg name="id" type="int" value="1"/>
		<constructor-arg name="address" type="java.lang.String" value="usa"/>
		<constructor-arg name="name" type="java.lang.String" value="tony"/>

	</bean>

</beans>